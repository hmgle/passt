lat	__LAT__ 100000 100000

tl	TCP/IPv4 to ns
ns1b	iperf3 -s1J -p 5221 | jq -rM ".end.sum_received.bits_per_second" >t1
hostb	iperf3 -c __ADDR4__ -p 5221 __OPTS__
sleep	12
ns1b	
ns1out	BW cat t1
bw	__BW__ 0.0 0.0
ns2b	iperf3 -s1J -p 5223 | jq -rM ".end.sum_received.bits_per_second" >t1
